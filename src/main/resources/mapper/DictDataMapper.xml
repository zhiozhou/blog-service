<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.zhou.domain.dao.DictDataDAO">


    <select id="list" parameterType="DictDataDTO" resultType="DictDataPO">
        SELECT
            `code`,
            label
        FROM
            system_dict_data
        where
            dict_key = #{dictKey}
    </select>



</mapper>
